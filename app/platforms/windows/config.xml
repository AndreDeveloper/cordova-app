<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.apprev" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>App Rev</name>
    <description>
        Um aplicativo para atender a funcionalidade de inclusão de revs do sistema BVQC
    </description>
    <author email="andredeveloper59@gmail.com" href="http://github.com/andredeveloper">
        André Oliveira
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
